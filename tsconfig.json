{
    "compilerOptions": {
        "module": "esnext",
        "target": "es2017",
        "lib": [
            "ESNext"
        ],
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "allowJs": true
    },
    // "include": [
    //     "**/src/**/*"
    // ],
    "exclude": [
        "node_modules"
    ]
}